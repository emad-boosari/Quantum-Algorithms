        "# Calculate the dot product of the results\n",
        "def bdotz(b, z):\n",
        "    sum = 0\n",
        "    for i in range(len(b)):\n",
        "        sum += int(b[i]) * int(z[i])\n",
        "    return (sum % 2)"
