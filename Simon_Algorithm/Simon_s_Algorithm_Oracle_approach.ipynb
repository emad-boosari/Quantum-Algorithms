    {
      "cell_type": "code",
      "source": [
        "def oracle(s):\n",
        "  index = [] # list to track position of \"1\" in the s bitstring\n",
        "  ctrl = 0   # contrl qubit\n",
        "  s_inv = s[::-1] # reveresed form of the 's'\n",
        "  n = len(s)\n",
        "\n",
        "  oracle = QuantumCircuit(2*n)\n",
        "\n",
        "  for i in range(n):\n",
        "    if s[i] == '1':\n",
        "      index.append(i+n)\n",
        "\n",
        "  for i in range(n):\n",
        "    if s_inv[i] == '1':\n",
        "      ctrl =i\n",
        "      break\n",
        "\n",
        "################################################\n",
        "  # copying the first register on the second register\n",
        "  for qubit in range(n):\n",
        "    oracle.cx(qubit,qubit+3)\n",
        "\n",
        "  # doing the bitwise addition\n",
        "  for i in index:\n",
        "    oracle.cx(ctrl, i)\n",
        "\n",
        "  oracle.to_gate()\n",
        "  oracle.name = \"Oracle\"\n",
        "\n",
        "  return oracle"
      ],
      "metadata": {
        "id": "1wozYPh7Rp_e"
      },
      "execution_count": 94,
      "outputs": []
    },
